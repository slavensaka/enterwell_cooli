{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "dev-open": "start http://localhost:3000 && pnpm dev",
    "build": "next build",
    "build-analyze": "cross-env ANALYZE=true pnpm build",
    "export": "rimraf ./out && rimraf ./.next && cross-env NODE_ENV=production next build && next export",
    "start": "next start",
    "lint": "next lint",
    "test": "pnpm test:unit && pnpm test:component && pnpm test:e2e",
    "test:unit": "jest",
    "test:e2e": "playwright test",
    "test:e2e-open": "pnpm test:e2e --ui",
    "e2e-check": "node -e \"require('./helpers/ci/ScreenshotsCompare').comparePlaywright()\"",
    "test:component": "playwright test -c playwright-ct.config.ts",
    "test:component-open": "pnpm test:component --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "stories-check": "echo 'storycap is incompatible with Storybook 10. Use Chromatic or similar tool for visual regression testing.'",
    "feature:remove": "node ./helpers/featureSelect.ts",
    "db:seed": "tsx prisma/seed.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/cache": "11.14.0",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@mui/icons-material": "7.3.6",
    "@mui/material": "7.3.6",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "axios": "1.7.9",
    "clsx": "2.1.1",
    "colorette": "2.0.20",
    "mobx": "6.13.5",
    "mobx-react-lite": "4.1.1",
    "next": "16.1.0",
    "pg": "^8.17.1",
    "pixelmatch": "5.3.0",
    "pngjs": "7.0.0",
    "prisma": "^7.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hot-toast": "2.4.1",
    "sass": "1.83.1",
    "typescript": "5.7.3",
    "webpack": "5.97.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.17.0",
    "@next/bundle-analyzer": "16.1.0",
    "@next/eslint-plugin-next": "16.1.0",
    "@playwright/experimental-ct-react": "1.57.0",
    "@playwright/test": "1.57.0",
    "@storybook/addon-links": "10.1.11",
    "@storybook/nextjs-vite": "10.1.11",
    "@storybook/react": "10.1.11",
    "@svgr/webpack": "8.1.0",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.8",
    "@typescript-eslint/eslint-plugin": "8.19.0",
    "@typescript-eslint/parser": "8.19.0",
    "cross-env": "7.0.3",
    "dotenv": "^17.2.3",
    "eslint": "9.17.0",
    "eslint-config-next": "16.1.0",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "eslint-plugin-react": "7.37.0",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-storybook": "10.1.11",
    "fs-extra": "11.2.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "kill-port": "2.0.1",
    "postcss": "8.4.49",
    "puppeteer": "24.0.0",
    "rimraf": "6.0.1",
    "sass-loader": "14.2.1",
    "storybook": "10.1.11",
    "tsx": "^4.21.0",
    "webpack-dev-server": "5.2.0"
  }
}
